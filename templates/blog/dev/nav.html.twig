<nav aria-label="Page navigation example"><ul class="pagination">{% if paginator.hasPreviousPage %}<li class="prev border p-2"><a href="{{ path('blog_index_paginated', {page: paginator.previousPage}) }}" rel="previous"><i class="fa fw fa-long-arrow-left"></i> {{ 'paginator.previous'|trans }}</a></li>{% else %}<li class="prev disabled border p-2"><span><i class="fa fw fa-arrow-left"></i> {{ 'paginator.previous'|trans }}</span></li>{% endif %}for i in 1..paginator.lastPage %}{% if i == paginator.currentPage %}<li class="active border p-2"><span>{{ i }} <span class="sr-only">{{ 'paginator.current'|trans }}</span></span></li>{% else %}<li class=" border p-2"><a href="{{ path('blog_index_paginated', {page: i}) }}">{{ i }}</a></li>{% endif %}{% endfor %}if paginator.hasNextPage %}<li class="next border p-2"><a href="{{ path('blog_index_paginated', {page: paginator.nextPage}) }}" rel="next">{{ 'paginator.next'|trans }} <i class="fa fw fa-arrow-right"></i></a></li>{% else %}<li class="next disabled border p-2"><span{{ 'paginator.next'|trans }} <i class="fa fw fa-arrow-right"></i></span></li>{% endif %}</ul></nav>
		